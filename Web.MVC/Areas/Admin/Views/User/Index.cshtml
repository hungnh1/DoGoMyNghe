@model IEnumerable<EnF.AspNetUser>

@{
    ViewBag.Title = "Quản lý user";
}

<h2>@ViewBag.Title </h2>

<p>
    @*@Html.ActionLink("Thêm mới", "Create")*@
    <a href="/Account/Register">Thêm mới</a>
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        
        <td>
          
            @*@Html.ActionLink("Xem", "Details", new { id=item.Id }) |*@
            @*@Html.ActionLink("Xóa", "Delete", new { id=item.Id })*@
            <a href="/Admin/User/Delete/@item.Id" onclick="return ConfirmDel()">Xóa</a>
        </td>
    </tr>
}

</table>
<script type="text/javascript">
    function ConfirmDel() {
        var rs = confirm("Chắc chắn muốn xóa");
        if (rs == false) {
            return false;
        }

    }

</script>